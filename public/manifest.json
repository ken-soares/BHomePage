{
  "name": "BHomePage",
  "description": "A customisable homepage for new tabs",
  "version": "0.1",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
